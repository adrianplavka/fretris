{
  "name": "gittie",
  "version": "1.0.0",
  "description": "Project management & more, for starting developers.",
  "main": "index.js",
  "keywords": [
    "git",
    "management"
  ],
  "author": "Adrian Plavka",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "setupFiles": [
      "raf/polyfill"
    ],
    "verbose": true,
    "silent": true
  },
  "scripts": {
    "build:server": "webpack --config src/config/webpack.common.js",
    "start:server": "webpack --config src/config/webpack.common.js && node src/.dist/index.js",
    "start:server:dev": "webpack --config src/config/webpack.dev.js --watch",
    "build:client": "webpack --config src/client/web/config/webpack.prod.js",
    "start:client": "webpack-dev-server --open --config src/client/web/config/webpack.ws.dev.js",
    "test": "jest",
    "test:server": "jest --testPathIgnorePatterns=/client/",
    "test:client": "jest client.web"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.9",
    "@types/express": "^4.0.37",
    "@types/jest": "^21.1.1",
    "@types/lodash": "^4.14.76",
    "@types/node": "^8.0.31",
    "@types/react-dom": "^15.5.5",
    "@types/react-redux": "^5.0.9",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/redux-logger": "^3.0.4",
    "@types/superagent": "^3.5.5",
    "@types/supertest": "^2.0.3",
    "babel-minify-webpack-plugin": "^0.2.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-cli": "^21.2.1",
    "jest-enzyme": "^4.0.0",
    "nodemon-webpack-plugin": "^0.1.4",
    "raf": "^3.3.2",
    "react-test-renderer": "16",
    "style-loader": "^0.18.2",
    "superagent": "^3.6.2",
    "supertest": "^3.0.0",
    "ts-jest": "^21.0.1",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.3",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "express": "^4.16.1",
    "lodash": "^4.17.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6"
  }
}